
<%- include('header') -%>

    <div class="box" id="heading">
        <h1>It's a <%= listTitel %></h1> 
    </div>

   <div class="box">
    <ul>
        <% for(var i = 0 ; i <  newlistItems.length ; i++) { %>
             <div class="item">
                <input type="checkbox"  >
              <p> <%=newlistItems[i] %> </p> 
             </div>
        
        <% } %>
      </ul>

      <form class="item" action="/" method="post">
        <input type="text" name="newItem"  placeholder="New Item" autocomplete="off" >
        <button type="submit" name="list" value= <%= listTitel %> >+</button>
     </form>

   </div>
  
   <%- include('footer') -%>
   